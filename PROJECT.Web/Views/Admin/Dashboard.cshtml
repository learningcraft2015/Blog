@{
    ViewBag.Title = "Admin Dashboard";

}


<div class="row">
    <div class="box col-md-12">
        <div class="box-inner">
            <div class="box-header well" data-original-title="">
                <h2><i class="glyphicon glyphicon-star-empty"></i> Dashboard</h2>

            </div>
            <div class="box-content">
                <!-- put your content here -->
                <div class="row">
                    <div class="col-lg-3 col-md-6">
                        <div class="panel panel-green">
                            <div class="panel-heading" style="background-color:orange;color:white">
                                <div class="row">
                                    <div class="col-xs-3">
                                        <i class="fa fa-dashboard fa-5x"></i>
                                    </div>
                                    <div class="col-xs-9 text-right">
                                        <div class="huge">(5)</div>
                                        <div style="font-size:25px">RESULT</div>
                                    </div>
                                </div>
                            </div>
                            <a href="@Url.Action("Dashboard", "Admin")">
                                <div class="panel-footer">
                                    <span class="pull-left">Result List</span>
                                    <span class="pull-right"><i class="fa fa-arrow-circle-o-right"></i></span>
                                    <div class="clearfix"></div>
                                </div>
                            </a>
                        </div>
                    </div>

                    <div class="col-lg-3 col-md-6">
                        <div class="panel panel-green">
                            <div class="panel-heading" style="background-color:darkred;color:white">
                                <div class="row">
                                    <div class="col-xs-3">
                                        <i class="fa fa-linux fa-5x"></i>
                                    </div>
                                    <div class="col-xs-9 text-right">
                                        <div class="huge">(10)</div>
                                        <div style="font-size:25px">NOTIFICATION</div>
                                    </div>
                                </div>
                            </div>
                            <a href="@Url.Action("Dashboard", "Admin")">
                                <div class="panel-footer">
                                    <span class="pull-left">Notification List</span>
                                    <span class="pull-right"><i class="fa fa-arrow-circle-o-right"></i></span>
                                    <div class="clearfix"></div>
                                </div>
                            </a>
                        </div>
                    </div>


                    <div class="col-lg-3 col-md-6">
                        <div class="panel panel-green">
                            <div class="panel-heading" style="background-color:black;color:white">
                                <div class="row">
                                    <div class="col-xs-3">
                                        <i class="fa fa-gittip fa-5x"></i>
                                    </div>
                                    <div class="col-xs-9 text-right">
                                        <div class="huge">(12)</div>
                                        <div style="font-size:25px">FEEDBACK</div>
                                    </div>
                                </div>
                            </div>
                            <a href="@Url.Action("Dashboard", "Admin")">
                                <div class="panel-footer">
                                    <span class="pull-left">Feedback List</span>
                                    <span class="pull-right"><i class="fa fa-arrow-circle-o-right"></i></span>
                                    <div class="clearfix"></div>
                                </div>
                            </a>
                        </div>
                    </div>

                    <div class="col-lg-3 col-md-6">
                        <div class="panel panel-green">
                            <div class="panel-heading" style="background-color:deepskyblue;color:white">
                                <div class="row">
                                    <div class="col-xs-3">
                                        <i class="fa fa-user fa-5x"></i>
                                    </div>
                                    <div class="col-xs-9 text-right">
                                        <div class="huge">(4)</div>
                                        <div style="font-size:25px">MEMBERS</div>
                                    </div>
                                </div>
                            </div>
                            <a href="@Url.Action("Dashboard", "Admin")">
                                <div class="panel-footer">
                                    <span class="pull-left">Member List</span>
                                    <span class="pull-right"><i class="fa fa-arrow-circle-o-right"></i></span>
                                    <div class="clearfix"></div>
                                </div>
                            </a>
                        </div>
                    </div>

                </div>

            </div>
        </div>
    </div>
</div>

<!-- put your content here -->
<div class="row">
    <div class="box col-md-6">
        <div class="box-inner">
            <div class="box-header well" data-original-title="">
                <h2><i class="glyphicon glyphicon-th"></i>Monthly Average Temperature</h2>
                
            </div>
            <div class="box-content">
                <div class="row">
                    <div class="col-md-12">

                        <img src="~/Content/images/DashboardLeftList.JPG" alt="No List" width="100%" height="100%" />
                    </div>
                   
                </div>
            </div>
        </div>
    </div>
    <!--/span-->
    <div class="box col-md-6">
        <div class="box-inner">
            <div class="box-header well" data-original-title="">
                <h2><i class="glyphicon glyphicon-th"></i>Monthly Average Rainfall</h2>
                
            </div>
            <div class="box-content">
                <div class="row">
                    <div class="col-md-12">
                   
                        <img src="~/Content/images/DashboardRightChart.jpeg" alt="No Chart" width="100%" height="100%" />
                    </div>
                   
                </div>
            </div>
        </div>
    </div>
    <!--/span-->
</div><!--/row-->




